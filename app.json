{
  "expo": {
    "name": "React Native Demo Wallet",
    "slug": "react-native-demo-wallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "react-native-demo-wallet",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "xyz.tkhqlabs.passkeyapp",
      "associatedDomains": ["webcredentials:passkeyapp.tkhqlabs.xyz"],
      "appleTeamId": "74V7N6MW3W",
      "entitlements": {
        "com.apple.developer.applesignin": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "xyz.tkhqlabs.passkeyapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-gradle-ext-vars",
        {
          "androidXBrowser": "1.8.0"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35
          }
        }
      ],
      "expo-apple-authentication"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
